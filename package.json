{
  "name": "uvplayer-lite",
  "productName": "uvplayer-lite",
  "version": "0.1.6",
  "description": "Universal video player lite version",
  "main": "./out/main/index.js",
  "author": "identity invest",
  "homepage": "https://github.com/ii-ua/uvplayer_lite",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "start:dev": "electron-vite dev --watch",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win --config --publish never",
    "build:linux": "npm run build && electron-builder --linux --config --publish never",
    "bump": "npm version patch -m \"Release %s\" && git push && git push --tags",
    "postinstall": "electron-builder install-app-deps"
  },
  "license": "MIT",
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@fontsource/open-sans": "^5.0.29",
    "@google-cloud/storage": "^7.12.1",
    "@vidstack/react": "^1.9.8",
    "antd": "^5.20.2",
    "axios": "^1.7.4",
    "axios-retry": "^4.5.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "electron-log": "^5.1.7",
    "electron-store": "^10.0.0",
    "googleapis": "^142.0.0",
    "hls.js": "^1.5.15",
    "modern-normalize": "^3.0.0",
    "moment": "^2.30.1",
    "node-schedule": "^2.1.1",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.26.1",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@sentry/vite-plugin": "^2.22.2",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "latest",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.2"
  }
}
