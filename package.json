{
  "name": "uvplayer_lite",
  "productName": "uvplayer_lite",
  "version": "0.0.1",
  "description": "Universal video player lite version",
  "main": "./out/main/index.js",
  "author": "identity invest",
  "homepage": "https://github.com/ii-ua/uvplayer_lite",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "start:dev": "electron-vite dev --watch",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win --config --publish never",
    "build:linux": "npm run build && electron-builder --linux --config --publish never",
    "deploy:win": "npm run build && electron-builder --win --config --publish always",
    "deploy:linux": "npm run build && electron-builder --linux --config --publish always",
    "bump": "npm version patch -m \"Release %s\" && git push && git push --tags",
    "postinstall": "electron-builder install-app-deps"
  },
  "license": "MIT",
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.1",
    "@fontsource/open-sans": "^5.0.17",
    "@sentry/electron": "^4.14.0",
    "@sentry/react": "^7.83.0",
    "@vidstack/react": "^1.5.5",
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0",
    "dotenv": "^16.3.1",
    "electron-log": "^5.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "hls.js": "^1.4.12",
    "modern-normalize": "^2.0.0",
    "moment": "^2.29.4",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@sentry/vite-plugin": "^2.10.2",
    "@vitejs/plugin-react": "^4.2.0",
    "electron": "^27.1.2",
    "electron-builder": "^24.9.1",
    "electron-vite": "^1.0.29",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.3"
  }
}
